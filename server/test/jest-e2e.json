{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@generated/client$": "<rootDir>/../generated/prisma/client",
    "^src/(.*)$": "<rootDir>/../src/$1"
  }
}
